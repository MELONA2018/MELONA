    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddNoteActivity" />
        <activity android:name=".NotePreviewActivity" />
        <activity android:name=".CalendarMainActivity" />
        <activity android:name=".ListAct" />
        <activity android:name=".Manage" />
        <activity android:name=".Que01"></activity>
    </application>

</manifest>